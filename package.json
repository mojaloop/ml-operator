{
  "name": "@mojaloop/ml-operator",
  "version": "1.1.0-alpha.294",
  "main": "index.ts",
  "repository": "https://github.com/mojaloop/ml-operator",
  "author": "Matt de Haast <matt.dehaast@coil.com>",
  "license": "Apache 2.0",
  "scripts": {
    "audit:resolve": "SHELL=sh resolve-audit --production",
    "audit:check": "SHELL=sh check-audit --production",
    "build": "tsc && cp -R config dist",
    "build:api": "npm run build:api:inbound && npm run build:api:outbound",
    "docker:build": "docker build -t mojaloop/ml-operator:local .",
    "lint": "eslint ./src/**/*.ts *.js",
    "pretest": "echo \"pretest - TODO...\"",
    "release": "standard-version --releaseCommitMessageFormat 'chore(release): {{currentTag}} [skip ci]'",
    "release-new": "./scripts/_release.sh",
    "standard": "echo '\\033[1;33m  This project uses eslint instead of standard. Use `npm run lint` instead.'",
    "start": "ts-node --project ./tsconfig.json --files --require tsconfig-paths/register ./src/index.ts",
    "dev": "ts-node-dev --project ./tsconfig.json --files --require tsconfig-paths/register src/index.ts start",
    "test": "npm run test:unit",
    "test:coverage": "jest --coverage  --testMatch '**/src/**/*.unit.ts'",
    "test:coverage-check": "jest --coverage --testMatch '**/src/**/*.unit.ts'",
    "test:integration": "jest --config './jest.integration.config.js' --testMatch '**/src/**/*.integration.ts'",
    "test:unit": "jest --collect-coverage=false --testMatch '**/src/**/*.unit.ts'",
    "updates:check": "npm-check-updates",
    "updates:update": "npm-check-updates -u && npm install",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@koa/router": "^9.3.1",
    "@kubernetes/client-node": "^0.12.0",
    "@mojaloop/central-services-error-handling": "^11.3.0",
    "@mojaloop/central-services-health": "^13.0.0",
    "@mojaloop/central-services-logger": "^10.6.0",
    "convict": "^6.0.0",
    "cron": "^1.8.2",
    "got": "^11.5.1",
    "koa": "^2.13.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/format": "^11.0.0",
    "@commitlint/lint": "^11.0.0",
    "@commitlint/load": "^11.0.0",
    "@types/convict": "^5.2.1",
    "@types/cron": "^1.7.2",
    "@types/hapi": "^18.0.3",
    "@types/hapi__hapi": "^19.0.4",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__vision": "^5.5.1",
    "@types/jest": "26.0.10",
    "@types/node": "^14.6.0",
    "@types/rc": "^1.1.0",
    "@types/redis-mock": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "add": "^2.0.6",
    "eslint": "7.7.0",
    "eslint-config-standard": "14.1.1",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-cucumber": "^1.4.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "^4.2.5",
    "jest": "26.4.1",
    "jest-cucumber": "^3.0.0",
    "jest-junit": "11.1.0",
    "lint-staged": "^10.2.11",
    "multi-file-swagger": "^2.3.0",
    "nodemon": "^2.0.4",
    "npm-audit-resolver": "2.2.1",
    "npm-check-updates": "7.1.1",
    "redis-mock": "^0.51.0",
    "source-map-support": "0.5.19",
    "standard-version": "^9.3.0",
    "swagger-cli": "^4.0.4",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.0.2"
  }
}
